.vertical-middle{position:absolute;top:50%;transform:translate(0,-50%);}
.box-center{position:relative;left:50%;transform:translate(-50%,0);}
.official-tip{width:24rpx;padding-bottom:3rpx;line-height:24rpx;vertical-align:-6rpx;}

/*switch*/
.switch{position:relative;width:81rpx;height:37rpx;padding:4rpx 7rpx;border:#e1c28f 1rpx solid;border-radius:100rpx;overflow:hidden;}
.switch::before{content:"";position:absolute;background-color:#fff;border-radius:100rpx;transition:all ease-out 0.3s;}
.switch::after{position:absolute;display:inline-block;content:"";width:47rpx;height:37rpx;background:#e1c28f url("https://m3.xiaoyu.com/img/wechatapp/xhb/icon.png") no-repeat;background-size:750rpx auto;border-radius:100rpx;transition:left 0.2s ease-out;}
.toggle-off::after{left:5rpx;background-position:-135rpx 10rpx;}
.toggle-on:after{left:38rpx;background-position:-185rpx 10rpx;}

/*闪动*/
.earth{position:absolute;top:40%;left:50%;width:530rpx;height:33%;margin-left:-265rpx;}
.earth-con{width:530rpx;height:530rpx;}
.unopened-icon{position:absolute;top:162rpx;left:72rpx;width:396rpx;height:184rpx;background:url("https://m3.xiaoyu.com/img/wechatapp/xhb/unopened-icon.png") no-repeat left top;background-size:100% 100%;z-index:2;}
.twinkling{position:absolute;background-color:#fff;border-radius:750rpx;animation-name:twinkling;animation-iteration-count:infinite;animation-timing-function:ease-in-out;}
 @keyframes twinkling{0%{opacity:0;}100%{opacity:1;}}

/*navigationBar*/
.navigationBar{position:fixed;top:0;left:0;right:0;height:44px;z-index:9999;line-height:40px;font-weight:600;}
